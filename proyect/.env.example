# --- Database ---
POSTGRES_DB=telemetry
POSTGRES_USER=telemetry
POSTGRES_PASSWORD=telemetry
DATABASE_URL=postgresql+psycopg://telemetry:telemetry@db:5432/telemetry

# --- API Keys ---
# Use keys you can rotate. Do NOT commit your real .env to GitHub.
READ_API_KEY=read_key_change_me
WRITE_API_KEY=write_key_change_me

# --- Retention ---
RETENTION_DAYS=30

# --- App ---
LOG_LEVEL=INFO
MAX_TAGS=20
MAX_TAG_KEY_LEN=50
MAX_TAG_VALUE_LEN=200
MAX_PAGE_SIZE=500


